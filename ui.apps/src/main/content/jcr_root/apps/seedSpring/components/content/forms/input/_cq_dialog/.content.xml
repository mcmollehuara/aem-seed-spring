<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Input"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/tabs" margin="{Boolean}false" type="nav" />
            <items jcr:primaryType="nt:unstructured">
				<tab1 jcr:primaryType="nt:unstructured" jcr:title="General" sling:resourceType="granite/ui/components/foundation/section">
                    <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" margin="{Boolean}false" type="nav"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <label
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Label"
                                        name="./label"/>  
                                    <value
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Value"
                                        name="./value"/>                                                                                                                                                                                                   
                                    <type
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Type"
                                        name="./type">
                                        <items jcr:primaryType="nt:unstructured">
                                            <text
                                                jcr:primaryType="nt:unstructured"
                                                text="text"
                                                value="text"/>
                                            <email
                                                jcr:primaryType="nt:unstructured"
                                                text="Email"
                                                value="email"/>	 
                                            <password
                                                jcr:primaryType="nt:unstructured"
                                                text="Password"
                                                value="password"/>                                                                                               
                                            <color
                                                jcr:primaryType="nt:unstructured"
                                                text="Color"
                                                value="color"/>																	
                                            <date
                                                jcr:primaryType="nt:unstructured"
                                                text="date"
                                                value="date"/>
                                            <datetime-local
                                                jcr:primaryType="nt:unstructured"
                                                text="datetime-local"
                                                value="datetime-local"/>															
				
                                            <!--<hidden
                                                jcr:primaryType="nt:unstructured"
                                                text="hidden"
                                                value="hidden"/>-->
                                            <!--<image
                                                jcr:primaryType="nt:unstructured"
                                                text="image"
                                                value="image"/>-->
                                            <number
                                                jcr:primaryType="nt:unstructured"
                                                text="Number"
                                                value="number"/>

                                            <radio
                                                jcr:primaryType="nt:unstructured"
                                                text="Radio"
                                                value="radio"/>
                                            <checkbox
                                                jcr:primaryType="nt:unstructured"
                                                text="Checkbox"
                                                value="checkbox"/>                                                
                                            <range
                                                jcr:primaryType="nt:unstructured"
                                                text="Range"
                                                value="range"/>                                                                                                                                                                                                																																
                                            <!--
                                            <search
                                                jcr:primaryType="nt:unstructured"
                                                text="search"
                                                value="search"/>-->
                                            <tel
                                                jcr:primaryType="nt:unstructured"
                                                text="Telephone"
                                                value="tel"/>                                          
                                            <time
                                                jcr:primaryType="nt:unstructured"
                                                text="time"
                                                value="time"/>
                                            <url                                            
                                                jcr:primaryType="nt:unstructured"
                                                text="Url"
                                                value="url"/>                                                 
                                            <week
                                                jcr:primaryType="nt:unstructured"
                                                text="Week"
                                                value="week"/>                                                 
                                        </items>
                                    </type>                                        
                                </items>

                            </column>
                        </items>
                </tab1>	
                <tab2 jcr:primaryType="nt:unstructured" jcr:title="Advanced" sling:resourceType="granite/ui/components/foundation/section">
                    <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns" margin="{Boolean}false" type="nav"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">                                       
                                    <name
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Name"
                                        name="./name"/>    
                                    <emptyText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Empty Text"
                                        name="./emptyText"/> 
                                    <disabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        fieldLabel="Disabled"
                                        text="Disabled"
                                        name="./disabled"/>    
                                    <required
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                        fieldLabel="Required"
                                        text="Required"
                                        name="./required"/>   
                                    <maxLenght
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Max Lenght"
                                        name="./maxLenght"/>                                                                                                                                     
                                </items>
                            </column>
                        </items>                
                </tab2>	
            </items>   
    </content>
</jcr:root>
